# Laravel project .gitignore

# Laravel & PHP
/vendor
/node_modules
/public/hot
/public/storage
/storage/*.key
/.env
/.phpunit.result.cache
/.php_cs.cache
/bootstrap/cache/*.php

# Ignore environment-specific files
Homestead.yaml
Homestead.json
/.vagrant

# Logs & system files
/storage/logs/*
!/storage/logs/.gitkeep
/storage/framework/cache/*
/storage/framework/sessions/*
/storage/framework/testing/*
/storage/framework/views/*
.DS_Store
Thumbs.db
desktop.ini

# NPM / Yarn
npm-debug.log*
yarn-debug.log*
yarn-error.log*
pnpm-debug.log*

# IDEs / editors
/.idea
/.vscode
*.sublime-project
*.sublime-workspace

# Composer
composer.phar

# Misc
.env.*.backup